{
  "name": "actionsflow-action",
  "version": "0.1.21",
  "private": true,
  "description": "Actionsflow action",
  "keywords": [
    "actionsflow"
  ],
  "author": "Owen Young <theowenyoung@gmail.com>",
  "homepage": "https://github.com/actionsflow/actionsflow/tree/master/packages/actionsflow-action",
  "license": "MIT",
  "main": "dist/src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actionsflow/actionsflow.git"
  },
  "scripts": {
    "start": "tsc -w -p tsconfig.production.json",
    "build": "npx ncc build src/index.ts",
    "test": "jest --passWithNoTests",
    "lint": "eslint --fix ./src && prettier --write  --loglevel warn ./src",
    "prepublishOnly": "npm run build",
    "hooks:pre-commit": "rimraf ./dist && npm run build && git add dist/*"
  },
  "bugs": {
    "url": "https://github.com/actionsflow/actionsflow/issues"
  },
  "dependencies": {
    "@actions/core": "^1.2.4",
    "actionsflow": "^0.1.21"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.23.0"
  }
}
